{
  "compilerOptions": {
    "outDir": "./built",
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
, "src/components/stories/Button/ButtonGroup.stories.jsx", "src/context/ColorModeContext.tsx", "src/layouts/AuthLayout.tsx", "src/components/styles/Layouts/AuthLayout/AuthLayoutPage.styled.js"  ]
}